{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "dev": "nodemon index.js --exec babel-node",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "clean:build": "rimraf dist/",
    "test": "cross-env NODE_ENV=test nyc --reporter=html mocha --require babel-core/register server/test/index.spec.js --exit && nyc report",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "undo:migrations": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrations": "node_modules/.bin/sequelize db:migrate",
    "seeders": "node_modules/.bin/sequelize db:seed:all",
    "pretest": "cross-env NODE_ENV=test npm run undo:migrations && cross-env NODE_ENV=test npm run migrations && cross-env NODE_ENV=test npm run seeders",
    "heroku-postbuild": "npm run migrations"
  },
  "engines": {
    "node": "8.11.4"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "slug": "^0.9.1",
    "swagger-jsdoc": "^3.0.2",
    "swagger-ui-express": "^4.0.0",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.3",
    "nyc": "^13.0.1"
  }
}
